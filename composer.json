{
    "name": "vigihdev/wp-assets",
    "description": "Vigih Dev Wp Assets",
    "homepage": "https://github.com/vigihdev/wp-assets",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "wordpress",
        "seo",
        "plugin",
        "vigihdev",
        "Assets"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Vigih Dev",
            "email": "vigihdev@gmail.com",
            "role": "developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.thrubus.id"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/filesystem": "^6.4",
        "vigihdev/support": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "vigihdev/wp-kernel": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Vigihdev\\WpAssets\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vigihdev\\WpAssets\\Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0,
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "scripts": {
        "test": "phpunit",
        "dev": "php -S localhost:5377 local-dev/index.php"
    }
}
